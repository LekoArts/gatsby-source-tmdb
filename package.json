{
  "name": "gatsby-source-tmdb",
  "version": "1.0.2",
  "description": "Source from The Movie Database (TMDb) in Gatsby",
  "main": "index.js",
  "author": "LekoArts <hello@lekoarts.de>",
  "repository": {
    "url": "https://github.com/LekoArts/gatsby-source-tmdb"
  },
  "homepage": "https://tmdb.lekoarts.de",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "gatsby-source-filesystem": "^2.0.20",
    "limits.js": "^0.1.4",
    "moviedb-promise": "^1.3.2",
    "progress": "^2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-preset-gatsby-package": "^0.1.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-prettier": "^3.0.1",
    "is-ci-cli": "^1.1.1",
    "jest": "^24.1.0",
    "jest-junit": "^6.2.1",
    "prettier": "^1.15.3"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "tmdb"
  ],
  "peerDependencies": {
    "gatsby": ">=2.0.0"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "lint": "is-ci \"lint:ci\" \"lint:dev\"",
    "lint:dev": "eslint . --ext .js --ignore-path .gitignore",
    "lint:ci": "yarn lint:dev --format junit -o reports/junit/js-lint-results.xml",
    "lint:fix": "eslint . --ext .js --fix --ignore-path .gitignore",
    "test": "is-ci \"test:ci\" \"test:watch\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --verbose",
    "test:ci": "yarn test:coverage --ci --runInBand --testResultsProcessor=\"jest-junit\"",
    "clear": "jest --clearCache"
  }
}
