{
  "name": "gatsby-source-tmdb",
  "version": "2.0.0",
  "description": "Source from The Movie Database (TMDb) in Gatsby",
  "main": "index.js",
  "engines": {
    "node": ">=10.13.0"
  },
  "author": "LekoArts <hello@lekoarts.de>",
  "repository": {
    "url": "https://github.com/LekoArts/gatsby-source-tmdb"
  },
  "bugs": {
    "url": "https://github.com/LekoArts/gatsby-source-tmdb/issues"
  },
  "homepage": "https://tmdb.lekoarts.de",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "gatsby-source-filesystem": "^2.7.0",
    "got": "^11.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@types/got": "^9.6.11",
    "babel-preset-gatsby-package": "^0.8.0",
    "cross-env": "^7.0.3",
    "gatsby": "^2.28.0",
    "typescript": "^4.1.2"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "tmdb",
    "moviedb",
    "themoviedb",
    "themoviedatabase"
  ],
  "peerDependencies": {
    "gatsby": ">=2.0.0"
  },
  "scripts": {
    "build": "babel src --out-dir lib/ --ignore \"**/__tests__\" --ignore \"**/__mocks__\" --extensions \".ts\"",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir lib/ --ignore \"**/__tests__\" --extensions \".ts\""
  }
}
